{
    "manifest_version": 3,
    "name": "Smart Font Substitution",
    "version": "1.0",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }]
  }