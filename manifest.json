{
    "manifest_version": 3,
    "name": "Custom Font+",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start" 
    }]
  }